<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="${relativePackage}.${activityClass}" >

    <Spinner
        android:id="@+id/AdecCmbSuspension"
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="16dp" />

    <Spinner
        android:id="@+id/AdecCmbTubo"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_alignLeft="@+id/AdecCmbSuspension"
        android:layout_below="@+id/AdecCmbSuspension" />

    <Spinner
        android:id="@+id/AdecCmbArmario"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_alignLeft="@+id/AdecCmbTubo"
        android:layout_below="@+id/AdecCmbTubo" />

    <Spinner
        android:id="@+id/AdecCmbSoporte"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_alignLeft="@+id/AdecCmbArmario"
        android:layout_below="@+id/AdecCmbArmario" />

    <Spinner
        android:id="@+id/AdecCmbTierra"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_alignLeft="@+id/AdecCmbSoporte"
        android:layout_below="@+id/AdecCmbSoporte" />

    <Spinner
        android:id="@+id/AdecCmbAcometida"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_alignLeft="@+id/AdecCmbTierra"
        android:layout_below="@+id/AdecCmbTierra" />

    <Spinner
        android:id="@+id/AdecCmbCaja"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_alignLeft="@+id/AdecCmbAcometida"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/AdecCmbAcometida" />

    <Spinner
        android:id="@+id/AdecCmbMedidor"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_alignLeft="@+id/AdecCmbCaja"
        android:layout_below="@+id/AdecCmbCaja" />

    <EditText
        android:id="@+id/AdecTxtOtros"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_alignLeft="@+id/AdecCmbMedidor"
        android:layout_below="@+id/AdecCmbMedidor"
        android:ems="10" >

        <requestFocus />
    </EditText>

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/AdecCmbTubo"
        android:layout_alignParentLeft="true"
        android:layout_alignTop="@+id/AdecCmbSuspension"
        android:layout_toLeftOf="@+id/AdecCmbSuspension"
        android:gravity="right|center_vertical"
        android:text="@string/AdecLblSuspension"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/AdecCmbArmario"
        android:layout_alignParentLeft="true"
        android:layout_alignTop="@+id/AdecCmbTubo"
        android:layout_toLeftOf="@+id/AdecCmbTubo"
        android:gravity="right|center_vertical"
        android:text="@string/AdecLblTubo"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/AdecCmbSoporte"
        android:layout_alignParentLeft="true"
        android:layout_alignRight="@+id/textView2"
        android:layout_alignTop="@+id/AdecCmbArmario"
        android:gravity="right|center_vertical"
        android:text="@string/AdecLblArmario"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/AdecCmbTierra"
        android:layout_alignParentLeft="true"
        android:layout_alignTop="@+id/AdecCmbSoporte"
        android:layout_toLeftOf="@+id/AdecCmbSoporte"
        android:gravity="right|center_vertical"
        android:text="@string/AdecLblSoporte"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/AdecCmbAcometida"
        android:layout_alignParentLeft="true"
        android:layout_alignRight="@+id/textView4"
        android:layout_alignTop="@+id/AdecCmbTierra"
        android:gravity="right|center_vertical"
        android:text="@string/AdecLblTierra"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/AdecCmbCaja"
        android:layout_alignParentLeft="true"
        android:layout_alignTop="@+id/AdecCmbAcometida"
        android:layout_toLeftOf="@+id/AdecCmbAcometida"
        android:gravity="right|center_vertical"
        android:text="@string/AdecLblAcometida"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/AdecCmbMedidor"
        android:layout_alignParentLeft="true"
        android:layout_alignRight="@+id/textView6"
        android:layout_alignTop="@+id/AdecCmbCaja"
        android:gravity="right|center_vertical"
        android:text="@string/AdecLblCaja"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/AdecTxtOtros"
        android:layout_alignParentLeft="true"
        android:layout_alignRight="@+id/textView7"
        android:layout_alignTop="@+id/AdecCmbMedidor"
        android:gravity="right|center_vertical"
        android:text="@string/AdeclblMedidor"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/AdecTxtOtros"
        android:layout_alignParentLeft="true"
        android:layout_alignRight="@+id/textView8"
        android:layout_alignTop="@+id/AdecTxtOtros"
        android:gravity="right|center_vertical"
        android:text="@string/AdecLblOtros"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <Button
        android:id="@+id/AdecBtnRegistrar"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="180dp"
        android:layout_height="35dp"
        android:layout_below="@+id/AdecTxtOtros"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="22dp"
        android:text="@string/AdecBtnRegistrar" />

</RelativeLayout>
