<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="${relativePackage}.${activityClass}" >

    <Spinner
        android:id="@+id/ContadorCmbMarca"
        android:layout_width="200dp"
        android:layout_height="25dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="10dp" />

    <Spinner
        android:id="@+id/ContadorCmbTipo"
        android:layout_width="match_parent"
        android:layout_height="25dp"
        android:layout_alignLeft="@+id/ContadorCmbMarca"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/ContadorCmbMarca" />

    <Spinner
        android:id="@+id/CensoCmbTipo"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_alignLeft="@+id/ContadorCmbTipo"
        android:layout_below="@+id/ContadorCmbTipo" />

    <Spinner
        android:id="@+id/CensoCmbCantidad"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_alignLeft="@+id/CensoCmbTipo"
        android:layout_below="@+id/CensoCmbTipo" />

    <EditText
        android:id="@+id/ContadorTxtSerie"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_alignLeft="@+id/CensoCmbCantidad"
        android:layout_below="@+id/CensoCmbCantidad"
        android:ems="10"
        android:inputType="text"
        android:textSize="@dimen/Text12pts" >

        <requestFocus />
    </EditText>

    <Spinner
        android:id="@+id/CensoCmbVatios"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_alignLeft="@+id/ContadorTxtSerie"
        android:layout_below="@+id/ContadorTxtSerie" />

    <TextView
        android:id="@+id/SellosLblTipoIngreso"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/ContadorCmbTipo"
        android:layout_alignParentLeft="true"
        android:layout_alignTop="@+id/ContadorCmbMarca"
        android:layout_toLeftOf="@+id/ContadorCmbMarca"
        android:gravity="right|center_vertical"
        android:text="@string/SellosLblTipoIngreso"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/SellosLblTipoSello"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/CensoCmbTipo"
        android:layout_alignParentLeft="true"
        android:layout_alignTop="@+id/ContadorCmbTipo"
        android:layout_toLeftOf="@+id/ContadorCmbTipo"
        android:gravity="right|center_vertical"
        android:text="@string/SellosLblTipoSello"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/SellosLblUbicacion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/CensoCmbCantidad"
        android:layout_alignParentLeft="true"
        android:layout_alignRight="@+id/SellosLblTipoSello"
        android:layout_alignTop="@+id/CensoCmbTipo"
        android:gravity="right|center_vertical"
        android:text="@string/SellosLblUbicacion"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/SellosLblColor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/ContadorTxtSerie"
        android:layout_alignParentLeft="true"
        android:layout_alignTop="@+id/CensoCmbCantidad"
        android:layout_toLeftOf="@+id/CensoCmbCantidad"
        android:gravity="right|center_vertical"
        android:text="@string/SellosLblColor"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/SellosLblSerie"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/CensoCmbVatios"
        android:layout_alignParentLeft="true"
        android:layout_alignTop="@+id/ContadorTxtSerie"
        android:layout_toLeftOf="@+id/ContadorTxtSerie"
        android:gravity="right|center_vertical"
        android:text="@string/SellosLblSerie"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/SellosLblrregularidad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/CensoCmbVatios"
        android:layout_alignParentLeft="true"
        android:layout_alignTop="@+id/CensoCmbVatios"
        android:layout_toLeftOf="@+id/CensoCmbVatios"
        android:gravity="right|center_vertical"
        android:text="@string/SellosLblIrregularidad"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <Button
        android:id="@+id/SellosBtnRegistrar"
        android:layout_width="150dp"
        android:layout_height="40dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="76dp"
        android:text="@string/SellosBtnRegistrar" />

    <HorizontalScrollView
        android:id="@+id/horizontalScrollView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/SellosBtnRegistrar"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/SellosLblrregularidad" >

        <ListView
            android:id="@+id/SellosLstSellos"
            android:layout_width="wrap_content"
            android:layout_height="match_parent" >
        </ListView>

    </HorizontalScrollView>

</RelativeLayout>
